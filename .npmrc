legacy-peer-deps=true
fetch-timeout=900000
fetch-retries=10
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
maxsockets=5
progress=false
loglevel=warn
prefer-offline=false
audit=false
fund=false
registry=https://registry.npmjs.org/
strict-ssl=true
# Prevent npm from hanging
prefer-online=false
# Set cache location
cache=/tmp/.npm
